<script>
        // Auto scroll function
        function autoScroll() {
          const tableContainer = document.querySelector('.table-container');
          const scrollStep = 1;
          let scrollTop = 0;
      
          function scroll() {
            scrollTop += scrollStep;
            tableContainer.scrollTop = scrollTop;
      
            if (scrollTop >= tableContainer.scrollHeight - tableContainer.clientHeight) {
              clearInterval(scrollInterval);
              setTimeout(autoScroll, 2000); // Delay before next scroll
            }
          }
      
          // Start auto scrolling
          const scrollInterval = setInterval(scroll, 50); // Scroll speed in milliseconds
      
          // Stop auto scrolling on manual scroll
          tableContainer.addEventListener('scroll', function() {
            clearInterval(scrollInterval);
          });
        }
      
        // Start auto scrolling
        autoScroll();
